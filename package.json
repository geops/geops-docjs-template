{
  "name": "geops-docjs-template",
  "version": "1.0.0",
  "description": "geOps theme for documentation.js",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist ./assets/bundle",
    "develop:assets": "webpack -w",
    "develop:theme": "documentation serve --watch **/_types/*.js **/lib/helper.js **/*._ --theme .",
    "develop:server": "reload -d ./dist",
    "lint": "eslint ."
  },
  "dependencies": {
    "anchor-js": "4.1.0",
    "concat-stream": "1.6.0",
    "documentation": "12.1.0",
    "github-slugger": "1.2.0",
    "highlight.js": "9.12.0",
    "lodash": "4.17.13",
    "normalize.css": "7.0.0",
    "vinyl": "2.1.0",
    "vinyl-fs": "3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "@babel/preset-env": "^7.8.3",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "npm-run-all": "4.1.2",
    "postcss-loader": "3.0.0",
    "reload": "2.4.0",
    "resolve-url-loader": "2.2.1",
    "style-loader": "0.20.1",
    "webpack": "3.11.0"
  }
}
